{
  "name": "bitmark-parser-ts",
  "version": "1.0.10",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitmark-standard/bitmark-parser-ts.git"
  },
  "main": "main.ts",
  "keywords": [
    "bitmark",
    "grammar",
    "parser"
  ],
  "description": "A parser for Bitmark text",
